{
  "expo": {
    "name": "Canopy Chronicles",
    "slug": "canopy-chronicles",
    "scheme": "canopy-chronicles",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/CC-Logo.png",
    "splash": {
      "image": "./assets/imgs/OnboardingBG.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.techtuned.canopychronicles"
    },
    "android": {
      "package": "com.techtuned.canopychronicles"
    },
    "description": "Canopy Chronicles app to track cannabis grows and activity.",
    "plugins": [
      "expo-secure-store"
    ]
  }
}
